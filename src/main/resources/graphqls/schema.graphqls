type Dog{
    id: ID!
    name: String!
    breed: String!
    origin: String!
}

type Query{
    findAllDogs: [Dog]!
    findDogById: String!
}

type Mutation{
    newDog(name: String!, breed: String!, origin: String!) : Dog!
    deleteDogBreed(breed: String!) : Boolean
    updateDogBreed(newName: String!, id: ID!) : Dog!
}